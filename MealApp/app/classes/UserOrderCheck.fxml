<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20" xmlns:fx="http://javafx.com/fxml" fx:controller="app.mealapp2.Controllers.UserOrderCheckController" prefWidth="400" prefHeight="400">
        <TextField fx:id="nameField" promptText="Enter name" />
        <TextField fx:id="surnameField" promptText="Enter surname" />
        <TextField fx:id="monthField" promptText="Enter month (MM)" />
        <Button text="Check Orders" onAction="#checkOrders"/>
        <Label fx:id="resultLabel" />
    </VBox>

